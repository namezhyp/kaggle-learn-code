   "source": [
    "import re\n",
    "\n",
    "html_content = \"<div id='songs-list'>...</div>\"  # 假设这是您的HTML内容\n",
    "\n",
    "# 使用正则表达式提取汉字和电话号码\n",
    "pattern = r'(\\w+)|(\\d{3}-\\d{4})'\n",
    "match_result = re.findall(pattern, html_content)\n",
    "\n",
    "# 输出结果\n",
    "print(match_result)"
   ]
  }
